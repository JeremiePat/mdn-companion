{
  "camelcase": true,
  "curly"    : true,
  "forin"    : false,
  "latedef"  : "nofunc",
  "newcap"   : false,
  "noarg"    : true,
  "nonew"    : true,
  "quotmark" : "single",
  "undef"    : true,
  "unused"   : true,
  "strict"   : true,
  "trailing" : true,

  "eqnull"   : true,
  "esnext"   : true,
  "expr"     : true,
  "globalstrict": true,

  "maxerr"   : 100,
  "regexdash": true,
  "laxcomma" : false,
  "proto"    : true,

  "browser"  : true,
  "devel"    : true,
  "nonstandard": false,
  "worker"   : true,
  "predef": [
    "require",
    "exports"
  ]
}
